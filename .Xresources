! ~/.Xresources

!!! Fonts
URxvt*font:    xft:Dejavu Sans Mono:size=9
Xft.dpi:       96
Xft.antialias: true
Xft.rgba:      rgb
Xft.hinting:   true
Xft.hintstyle: hintslight
Xft.autohint:  false
Xft.lcdfilter: lcddefault

!!! Disable scrollbar
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.scrollBar_floating: false
URxvt.scrollstyle: rxvt

!!! Theme ("Tango-Dark")
URxvt*background: #000000
URxvt*foreground: #B2B2B2
*color0:  #2E3436
*color8:  #555753
*color1:  #a40000
*color9:  #EF2929
*color2:  #4E9A06
*color10: #8AE234
*color3:  #C4A000
*color11: #FCE94F
*color4:  #3465A4
*color12: #729FCF
*color5:  #75507B
*color13: #AD7FA8
*color6:  #ce5c00
*color14: #fcaf3e
*color7:  #babdb9
*color15: #EEEEEC
